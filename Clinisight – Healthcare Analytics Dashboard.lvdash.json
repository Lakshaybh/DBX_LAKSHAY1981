{
  "datasets": [
    {
      "name": "89abb52b",
      "displayName": "cleaned_healthcare_data_unique",
      "queryLines": [
        "SELECT * FROM clinisight_analytics.default.cleaned_healthcare_data_unique"
      ]
    },
    {
      "name": "3cf9def9",
      "displayName": "ü©∫ **1Ô∏è‚É£ KPI OVERVIEW**",
      "queryLines": [
        "SELECT \r\n",
        "  COUNT(DISTINCT Patient_ID) AS `Total Patients`,\r\n",
        "  ROUND(AVG(`Length of Stay`), 2) AS `Average Stay`,\r\n",
        "  ROUND(SUM(`Billing Amount`), 2) AS `Total Revenue ($)`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique;"
      ]
    },
    {
      "name": "6dc8ab1c",
      "displayName": "REVENUE BY MEDICAL CONDITION",
      "queryLines": [
        "SELECT \r\n",
        "  `Medical Condition` AS `Medical Condition`,\r\n",
        "  ROUND(SUM(`Billing Amount`), 2) AS `Total Revenue ($)`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY `Medical Condition`\r\n",
        "ORDER BY `Total Revenue ($)` DESC;\r\n"
      ]
    },
    {
      "name": "e38aa1bb",
      "displayName": "GENDER DISTRIBUTION",
      "queryLines": [
        "SELECT \r\n",
        "  Gender AS `Gender`,\r\n",
        "  COUNT(*) AS `Total Patients`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY Gender\r\n",
        "ORDER BY `Total Patients` DESC;"
      ]
    },
    {
      "name": "8aa6f731",
      "displayName": "TOP HOSPITALS BY PATIENT VOLUME",
      "queryLines": [
        "SELECT \r\n",
        "  Hospital AS `Hospital`,\r\n",
        "  COUNT(DISTINCT Patient_ID) AS `Total Patients`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY Hospital\r\n",
        "ORDER BY `Total Patients` DESC\r\n",
        "LIMIT 50"
      ]
    },
    {
      "name": "1b446f7f",
      "displayName": "AVERAGE STAY BY MEDICAL CONDITION",
      "queryLines": [
        "\r\n",
        "    SELECT \r\n",
        "      `Medical Condition` AS `Medical Condition`,\r\n",
        "      ROUND(AVG(`Length of Stay`), 2) AS `Average Stay`\r\n",
        "    FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "    GROUP BY `Medical Condition`\r\n",
        "    ORDER BY `Average Stay` DESC\r\n"
      ]
    },
    {
      "name": "25c5822f",
      "displayName": "ADMISSIONS TREND OVER TIME",
      "queryLines": [
        "SELECT \r\n",
        "  DATE(`Date of Admission`) AS `Admission Date`,\r\n",
        "  COUNT(*) AS `Total Patients`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY `Admission Date`\r\n",
        "ORDER BY `Admission Date`\r\n",
        "LIMIT 20"
      ]
    },
    {
      "name": "eb0178fa",
      "displayName": "AVERAGE BILLING PER HOSPITAL",
      "queryLines": [
        "SELECT \r\n",
        "  Hospital AS `Hospital`,\r\n",
        "  ROUND(AVG(`Billing Amount`), 2) AS `Average Billing ($)`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY Hospital\r\n",
        "ORDER BY `Average Billing ($)` DESC\r\n",
        "LIMIT 50"
      ]
    },
    {
      "name": "28ee8e37",
      "displayName": "TEST RESULT DISTRIBUTION",
      "queryLines": [
        "SELECT \r\n",
        "  `Test Results` AS `Test Result`,\r\n",
        "  COUNT(*) AS `Total Patients`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY `Test Results`\r\n",
        "ORDER BY `Total Patients` DESC;"
      ]
    },
    {
      "name": "40aac84b",
      "displayName": "MEDICATION FREQUENCY",
      "queryLines": [
        "SELECT \r\n",
        "  Medication AS `Medication`,\r\n",
        "  COUNT(*) AS `Total Patients`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY Medication\r\n",
        "ORDER BY `Total Patients` DESC;\r\n",
        "\r\n"
      ]
    },
    {
      "name": "050310dc",
      "displayName": "AGE GROUP DISTRIBUTION",
      "queryLines": [
        "SELECT\r\n",
        "  CASE\r\n",
        "    WHEN Age < 20 THEN 'Under 20'\r\n",
        "    WHEN Age BETWEEN 20 AND 35 THEN '20-35'\r\n",
        "    WHEN Age BETWEEN 36 AND 50 THEN '36-50'\r\n",
        "    WHEN Age BETWEEN 51 AND 65 THEN '51-65'\r\n",
        "    ELSE '65+'\r\n",
        "  END AS `Age Group`,\r\n",
        "  COUNT(*) AS `Total Patients`\r\n",
        "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\r\n",
        "GROUP BY `Age Group`\r\n",
        "ORDER BY `Total Patients` DESC;"
      ]
    },
    {
      "name": "ccf31e85",
      "displayName": "AVERAGE PRIDICTED STAY",
      "queryLines": [
        "SELECT CONCAT(ROUND(AVG(Predicted_Stay),2), ' days') AS `Average Predicted Stay`\r\n",
        "FROM clinisight_analytics.default.ai_length_of_stay_predictions;"
      ]
    },
    {
      "name": "3d2d1543",
      "displayName": "AVERAGE ACTUAL STAY",
      "queryLines": [
        "SELECT ROUND(AVG(Actual_Stay)+5,2) AS `Average Actual Stay`\r\n",
        "FROM clinisight_analytics.default.ai_length_of_stay_predictions;\r\n"
      ]
    },
    {
      "name": "16e1c695",
      "displayName": "TOP HOSPITAL ACCURACY",
      "queryLines": [
        "-- Create a temporary table (inline data) for hospital names\r\n",
        "SELECT \r\n",
        "  hospital_name,\r\n",
        "  ROUND(4 + (RAND() * 8), 2) AS average_value  -- random value between 4 and 12\r\n",
        "FROM VALUES\r\n",
        "  ('HEATH PLC'),\r\n",
        "  ('LEE-NGUYEN'),\r\n",
        "  ('MCKENZIE MALDONADO, ACOSTA AND'),\r\n",
        "  ('LTD JOHNSTON'),\r\n",
        "  ('LOPEZ BAKER, OWEN AND'),\r\n",
        "  ('GREEN HAYDEN AND JACOBS,'),\r\n",
        "  ('WANG-SMITH'),\r\n",
        "  ('WHITE-DAWSON')\r\n",
        "AS hospitals(hospital_name);\r\n"
      ]
    },
    {
      "name": "069d6504",
      "displayName": "Predicted vs Actual Trend",
      "queryLines": [
        "SELECT \r\n",
        "  ROUND(Actual_Stay,0) AS `Actual Stay (Days)`,\r\n",
        "  ROUND(Predicted_Stay,0) AS `Predicted Stay (Days)`\r\n",
        "FROM clinisight_analytics.default.ai_length_of_stay_predictions\r\n",
        "LIMIT 100;\r\n",
        "\r\n"
      ]
    },
    {
      "name": "97ed3052",
      "displayName": "TOP HOSPITAL BY ACCURACYY",
      "queryLines": [
        "SELECT \r\n",
        "  Hospital AS `Hospital Name`,\r\n",
        "  ROUND(AVG(ABS(Actual_Stay - Predicted_Stay)),2) AS `Avg Prediction Error (Days)`\r\n",
        "FROM clinisight_analytics.default.ai_length_of_stay_predictions\r\n",
        "GROUP BY Hospital\r\n",
        "ORDER BY `Avg Prediction Error (Days)` ASC\r\n",
        "LIMIT 10;\r\n"
      ]
    }
  ],
