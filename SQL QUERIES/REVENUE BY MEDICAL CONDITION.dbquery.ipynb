   "source": [
    "%sql\n",
    "SELECT \n",
    "  `Medical Condition` AS `Medical Condition`,\n",
    "  ROUND(SUM(`Billing Amount`), 2) AS `Total Revenue ($)`\n",
    "FROM clinisight_analytics.default.cleaned_healthcare_data_unique\n",
    "GROUP BY `Medical Condition`\n",
    "ORDER BY `Total Revenue ($)` DESC;\n"
